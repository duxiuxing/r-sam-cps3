<?xml version='1.0' encoding='utf-8'?>
<WiiFileExporter description="导出到 Wii SD 卡的文件">
    <WiiRA_App name="FB Alpha 2012 CPS-3" folder="fbalpha2012_cps3"
        content_show_history="true">
        <Folder
            src="wii\remaps"
            dst="remaps\FB Alpha 2012 CPS-3"
        />
        <Folder
            src="wii\retroarch\system\cache"
            dst="retroarch\system\cache"
        />
        <File
            src="wii\wad\fbalpha2012_cps3\Capcom CPS-3 White [CPS3].wad"
            dst="wad\fbalpha2012_cps3\Capcom CPS-3 White [CPS3].wad"
        />
        <File
            src="wii\wad\fbalpha2012_cps3\Capcom CPS-3 Yellow [CPS3].wad"
            dst="wad\fbalpha2012_cps3\Capcom CPS-3 Yellow [CPS3].wad"
        />
    </WiiRA_App>
    <WiiRA_App name="JoJo's Venture" folder="jojo" rom_title="jojo">
